  "license": "ISC",
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "dependencies": {
    "@apollo/client": "^3.9.10",
    "react-helmet": "^6.1.0",
    "react-loader-spinner": "^6.1.6",
    "semantic-ui-css": "^2.5.0",
    "semantic-ui-react": "^2.1.5"
  }
}
